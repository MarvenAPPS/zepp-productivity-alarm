  "targets": {
    "amazfit-balance": {
      "module": {
        "page": {
          "pages": [
            "page/index",
            "page/alarms",
            "page/questions",
            "page/history",
            "page/settings",
            "page/alarm-trigger"
          ]
        }
      },
      "platforms": [
        {
          "name": "amazfit-balance",
          "deviceSource": 234,
          "st": "r",
          "dw": 466
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Productivity Alarm"
    },
    "fr-FR": {
      "appName": "Alarme de Productivité"
    },
    "ar-EG": {
      "appName": "منبه الإنتاجية"
    }
